/*
write a variable in each of below value type and print its value (try to not use 'var/final/const' keyword)
the final output may be like this

true
4
1.2
ok
[4,3,"ok",true]

 */

void main(List<String> args) {
  int r = 4;
  int f = 5;
  bool isOk = f > r;
  double v = 1.2;
  String m = 'ok';

  print(isOk);
  print(r);
  print(v);
  print(m);

  List rm = [4, 3, "ok", isOk];
  print(rm);
}
